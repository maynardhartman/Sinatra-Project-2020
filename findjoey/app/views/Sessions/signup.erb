<div class="bg-img"> 
  <form action="/sessions/signup" class="container" method="post">
    <h1>FindJoey - Login</h1>
    <label for="fname"><b>First Name:</b></label>
    <input type="text" placeholder="Enter First Name" name="fname" required>

    <label for="password"><b>Password</b></label>
    <input type="password" placeholder="Enter Password" name="password" required>

    <label for="email"><b>Email:<b/></label>
    <input type="text" placeholder="Enter Email:" name="email" required><br />

    <label for="addr_1"><b>Address:<b/></label>
    <input type="text" placeholder="Address #1" name="addr_1" required>

    <label for="addr_2"><b>Address:><b/></label>
    <input type="addr_2" placeholder="Address #2" name="addr_2"><br/>
    
    <label for="city"><b>City:><b/></label>
    <input type="city" placeholder="City" name="city">
    <label for="state"><b>State:><b/></label>
    <input type="state" placeholder="ST" name="state">
    <label for="zipcode"><b>Zip:><b/></label>
    <input type="zipcode" placeholder="Zip" name="zipcode"><br/>

    <label for="phone_1"><b>Phone:><b/></label>
    <input type="phone_1" placeholder="Phone:" name="phone_2"><br/>

    <label for="phone_2"><b>Phone:><b/></label>
    <input type="phone_2" placeholder="Phone:" name="phone_2"><br/>

        <label for="is_missing"><b>Has Missing Dog(s)?:</label>
        <input type="checkbox" name="is_missing" checked="true"><br />    

    <button type="submit" class="btn">Signup</button>
  </form>
</div>
fname: Faker::Name.first_name,
    lname: Faker::Name.last_name,
    addr_1: Faker::Address.street_address,
    addr_2: nil,
    city: Faker::Address.city,
    state: Faker::Address.state_abbr,
    zipcode: Faker::Address.zip_code,
    email: Faker::Internet.safe_email,
    phone_1: Faker::PhoneNumber.cell_phone,
    phone_2: Faker::PhoneNumber.cell_phone,
    is_missing: 1,
    account_verified: 1,
    account_suspended: 0,
    suspension_reason: "Actount Active",
    longitude: Faker::Address.longitude,
    latitude: Faker::Address.latitude,