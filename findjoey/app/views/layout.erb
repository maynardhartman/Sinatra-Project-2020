    <title>Find Joey&copy;</title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="/materialize/css/materialize.min.css"  media="screen,projection"/>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
  </head>
  <body>

  <style>
    body { text-align:center; }
  </style>

  <div class='container'>
    <!-- refactor this to enable placement under the line the error occurred on -->
    <% flash.keys.each do |type| %>
    <div data-alert class="flash <%= type %> alert-box radius">
    <div class="col s12 m6">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= type %></span>
          <p>An Error Has Occured of Type "<%= type %>"</p>
        </div>
        <div class="card-action">
          <a href="http://127.0.0.1:9393/session/login">Retry</a>
        </div>
      </div>
    </div>
    </div>
      <a href="127.0.0.1:9393" class="close">&times;</a>
    </div>
    <% end %>
  
        <%= yield %>

    <div class="container" style="text-align: center;">
      <footer class="branding">
        <small>&copy; 2020 <strong>FindJoey - Maynard J. Hartman, Jr.</strong></small>
      </footer>
    </div>
  </div>
    
    
  </div>
  
  </body>
