  <body>
    <style>
      body { text-align:center; }
    </style>
  <div class='container'>
    <!-- refactor this to enable placement under the line the error occurred on -->
    <% flash.keys.each do |type| %>
    <div data-alert class="flash alert-box radius">
    <div class="col s6 m6 l6 column-offset ">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= flash[type] %></span>
          <p>An Error Has Occured</p>
        </div>
        <div class="card-action">
          <a href="http://127.0.0.1:9393/sessions/login">Retry</a>
        </div>
      </div>
    </div>
  </div>
      <a href="127.0.0.1:9393" class="close">&times;</a>
  </div>
    <% end %>
  
        <%= yield %>

<footer class="page-footer">
          <div class="container">
            <div class="row col l6">
              <div class="col l6 s12">
                <h5 class="white-text">Find Joey</h5>
                <p class="grey-text text-lighten-4">A dog lost/found reporting website.</p>
              </div>
            </div>
          </div>
          <div class="footer-copyright">
